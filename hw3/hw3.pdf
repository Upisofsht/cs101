#include <stdio.h>
#include <time.h>
#include <stdlib.h>

void hanoi(int Height, char A, char B, char C) {
    if(Height == 1) {
        printf("Move sheet from %c to %c\n", A, C);
    }
    else {
        hanoi(Height-1, A, C, B);
        hanoi(1, A, B, C);
        hanoi(Height-1, B, A, C);
    }
}
int main()
{
    int Height;
	clock_t start,end;
    start=clock();
    scanf("%d",&Height);
    hanoi(Height,'A','B','C');
    end=clock();
    double diff = end - start;
    printf("  using %f sec", diff / CLOCKS_PER_SEC );
    
    
    return 0;
}